      {host_aws, "elastic-server"},
      {port, 9200},
      {index, "sumo_test"},
      {poolsize, 10}]
    }

  ]},
    {stores, [
      {es_account, sumo_store_elasticsearch, [
        {storage_backend, elasticsearch_backend},
        {index, "dev_sos_account_idx"}
      ]},
      {es_configuration, sumo_store_elasticsearch, [
